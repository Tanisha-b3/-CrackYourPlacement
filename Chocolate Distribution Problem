Chocolate Distribution Problem;-

class Solution
{
    public long findMinDiff (ArrayList<Integer> a, int n, int m)
    {
        Collections.sort(a);
        int j = m-1; 
        long min = Integer.MAX_VALUE;
        while(j<n){
            long diff = a.get(j)-a.get(j-(m-1));
            min = Math.min(min,diff);
           j++;
        }
        return max;
    }
}
